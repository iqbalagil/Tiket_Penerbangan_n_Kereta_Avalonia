<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
            xmlns:vm="clr-namespace:Tiket_Penerbangan_n_Kereta.ViewModel.Dashboard"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Tiket_Penerbangan_n_Kereta.View.Dashboard.DataAnalyticalView"
             x:CompileBindings="True"
             x:DataType="vm:DataAnalyticalViewModel">
    
    <Grid RowDefinitions="Auto,*">
       <StackPanel VerticalAlignment="Center"
                   HorizontalAlignment="Left"
                   Orientation="Vertical">
           <lvc:CartesianChart
               Margin="20"
               Series="{Binding Series}"
               YAxes="{Binding YAxes}"
               DrawMarginFrame="{Binding DrawMarginFrame}"
               ZoomMode="Both"
               TooltipPosition="Hidden"
               >
           </lvc:CartesianChart>
       </StackPanel>
   </Grid>
</UserControl>
